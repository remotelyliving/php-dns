{
    "name": "remotelyliving/php-dns",
    "description": "A php library for abstracting DNS querying",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "chthomas",
            "email": "christian.h.thomas@me.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0 || ^6.0",
        "psr/cache": "^1.0 || ^2.0 || ^3.0",
        "symfony/event-dispatcher": "^6.0 || ^5.0 || ^4.0 || ^3.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "guzzlehttp/promises": "^1.3",
        "spatie/dns": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "maglnet/composer-require-checker": "@stable",
        "squizlabs/php_codesniffer": "^3.3",
        "psy/psysh": "^0.9.9",
        "php-coveralls/php-coveralls": "^2.1",
        "symfony/cache": "^4.3",
        "vimeo/psalm": "^4.10",
        "rector/rector": "^0.12.8",
        "bmitch/churn-php": "^1.5",
        "brianium/paratest": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "RemotelyLiving\\PHPDNS\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RemotelyLiving\\PHPDNS\\Tests\\" : "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
