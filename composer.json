{
    "name": "remotelyliving/php-dns",
    "description": "A php library for abstracting DNS querying",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "chthomas",
            "email": "christian.h.thomas@me.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.3",
        "psr/cache": "^1.0",
        "symfony/event-dispatcher": "~4.0 || ~3.0",
        "psr/log": "^1.0",
        "guzzlehttp/promises": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "phpstan/phpstan": "^0.10.5",
        "maglnet/composer-require-checker": "^1.1",
        "squizlabs/php_codesniffer": "^3.3",
        "psy/psysh": "^0.9.9",
        "php-coveralls/php-coveralls": "^2.1",
        "symfony/cache": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "RemotelyLiving\\PHPDNS\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RemotelyLiving\\PHPDNS\\Tests\\" : "tests/"
        }
    }
}
